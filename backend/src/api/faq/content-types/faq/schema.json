{
  "kind": "collectionType",
  "collectionName": "faqs",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "Частый вопрос"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "answer": {
      "type": "richtext",
      "required": true
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "category": {
      "type": "string"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product"
    }
  }
}
